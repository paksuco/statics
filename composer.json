{
    "name": "paksuco/statics",
    "description": "WIP - Common static pages for Laravel (Faq, Blog, Menu Pages etc)",
    "keywords": [
        "laravel",
        "static",
        "pages",
        "common",
        "general-purpose"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Taha PAKSU",
            "email": "tpaksu@gmail.com",
            "homepage": "https://rettica.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": "^7.0|^8.0",
        "illuminate/config": "^7.0|^8.0",
        "illuminate/routing": "^7.0|^8.0",
        "illuminate/database": "^7.0|^8.0",
        "paksuco/table": "9999999-dev",
        "paksuco/settings": "9999999-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Paksuco\\Statics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Paksuco\\Statics\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Paksuco\\Statics\\StaticsServiceProvider"
            ],
            "aliases": {
                "Package": "Paksuco\\Statics\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
